{
  "appenders": {
    "console": {
      "type": "console"
    },
    "stdout": {
      "type": "stdout"
    },
    "production": {
      "type": "dateFile",
      "filename": "./logs/",
      "alwaysIncludePattern": true,
      "pattern": "SNSWeb-yyyy-MM-dd.log"
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "console"
      ],
      "replaceConsole": true,
      "level": "INFO",
      "pm2": true,
      "pm2InstanceVar": "INSTANCE_ID"
    }
  }
}